{
  "extends": ["next/core-web-vitals", "airbnb", "eslint-config-prettier"],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          ["src", "./src"],
          ["@/Common", "./src/components/Common/index"],
          ["@/lib", "./src/lib"],
          ["@/components", "./src/components"],
          ["@/slices", "./src/slices"],
          ["@/styles", "./styles"],
          ["@/hooks", "./src/hooks"],
          ["@/constants", "./src/constants"],
          ["tailwindConfig", "./tailwind.config.js"]
        ],
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
